{
  "schema_version": "v1",
  "name_for_human": "Stock Data",
  "name_for_model": "stockData",
  "description_for_human": "Get aggregate data for a specific stock",
  "description_for_model": "Get aggregate data for a specific stock",
  "auth": {
    "type": "none"
  },
  "api": {
    "type": "openapi",
    "url": "https://04917c44cbe3.ngrok.app/openapi.yaml"
  },
  "logo_url": "https://04917c44cbe3.ngrok.app/logo.jpg",
  "contact_email": "legal@example.com",
  "legal_info_url": "http://example.com/legal",
  "endpoints": [
    {
      "name": "Get Stock Aggregate",
      "route": "/stocks",
      "description": "Get stock aggregate data",
      "methods": ["GET"],
      "parameters": [
        {
          "name": "stocksTicker",
          "description": "The ticker symbol of the stock/equity",
          "required": true,
          "type": "string"
        },
        {
          "name": "multiplier",
          "description": "The size of the timespan multiplier",
          "required": true,
          "type": "string"
        },
        {
          "name": "timespan",
          "description": "The size of the time window",
          "required": true,
          "type": "string"
        },
        {
          "name": "from",
          "description": "The start of the aggregate time window",
          "required": true,
          "type": "string"
        },
        {
          "name": "to",
          "description": "The end of the aggregate time window",
          "required": true,
          "type": "string"
        },
        {
          "name": "adjusted",
          "description": "Whether or not the results are adjusted for splits",
          "required": false,
          "type": "boolean"
        },
        {
          "name": "sort",
          "description": "Sort the results by timestamp",
          "required": false,
          "type": "string"
        },
        {
          "name": "limit",
          "description": "Limits the number of base aggregates queried",
          "required": false,
          "type": "integer"
        }
      ]
    },
    {
      "name": "Predict Stock Price",
      "route": "/predict",
      "description": "Predict stock price",
      "methods": ["POST"],
      "request_body": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data_type": {
                  "type": "string",
                  "description": "The type of the data used for stock price prediction.",
                  "enum": ["open_prices", "close_prices", "high_prices", "low_prices", "volume_weighted_prices"]
                }
              },
              "required": ["data_type"]
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "OK",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/predictStockPriceResponse"
              }
            }
          }
        }
      }
    }
  ],
  "components": {
    "schemas": {
      "predictStockPriceRequest": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "type": "number"
            },
            "description": "The feature data for stock price prediction"
          }
        },
        "required": ["data"]
      },
      "predictStockPriceResponse": {
        "type": "object",
        "properties": {
          "prediction": {
            "type": "array",
            "items": {
              "type": "number"
            },
            "description": "The predicted stock prices"
          },
          "actual": {
            "type": "array",
            "items": {
              "type": "number"
            },
            "description": "The actual stock prices"
          }
        },
        "required": ["prediction", "actual"]
      }
    }
  }
}
